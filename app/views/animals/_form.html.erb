<%= form_for(@animal) do |f| %>
 

  <div class="field">
    <%= f.label :animalsname %><br>
    <%= f.text_field :animalsname %>
  </div>
  <div class="field">
    <%= f.label :npwsid %><br>
    <%= f.number_field :npwsid, step: 1 %>
  </div>
  <div class="field">
    <%= f.label :family %><br>
    <%= f.text_field :family %>
  </div>
   <div class="field">
    <%= f.label :code %><br>
    <%= f.text_field :code %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
